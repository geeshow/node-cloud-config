{
  "name": "nodejs-cloud-config",
  "version": "0.0.7",
  "description": "Initial environment configuration from remote repository",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "pub": "npm publish",
    "spring-test": "NODE_ENV=spring node -r ts-node/register src/index.ts",
    "git-test": "NODE_ENV=git node -r ts-node/register src/index.ts",
    "url-test": "NODE_ENV=url node -r ts-node/register src/index.ts",
    "test": "jest"
  },
  "files": [
    "lib",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "cloud",
    "config",
    "environment",
    "remote",
    "repository",
    "configuration",
    "nodejs"
  ],
  "author": "ken",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "@types/js-yaml": "^4.0.9",
    "dotenv": "^16.4.1",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.12",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/geeshow/node-cloud-config"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browser": {
    "fs": false
  },
  "readmeFilename": "README.md"
}
